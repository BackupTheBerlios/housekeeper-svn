<statcvs>
  <settings>
  	<localRepository>./src/java</localRepository>
    <include>./src/**/*.java</include>
    <modules type="map">
  		<setting key="GUI">src/java/**/swing/**/*</setting>
  		<setting key="Domain">src/java/**/domain/**/*</setting>
   		<setting key="Persistence">src/java/**/persistence/**/*</setting>
  		<setting key="Test">src/test/**</setting>
	</modules>
  </settings>
</statcvs>
  